<Window x:Class="VietOCR.HtmlHelpDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VietOCR"
        mc:Ignorable="d"
        Title="VietOCR.NET Help" Height="400" Width="600">
    <DockPanel>
        <StatusBar DockPanel.Dock="Bottom" >
            <StatusBarItem>
                <TextBlock Name="lblURL" />
            </StatusBarItem>
        </StatusBar>
        <WebBrowser x:Name="webBrowser" DockPanel.Dock="Top" Navigating="webBrowser_Navigating"/>
    </DockPanel>
</Window>
